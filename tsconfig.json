{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    // 下面这些保持原样
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "noImplicitAny": false
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}